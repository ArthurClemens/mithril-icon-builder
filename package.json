{
  "name": "mithril-icon-builder",
  "version": "0.0.1",
  "description": "Converts Material Design SVG icons to Mithril elements.",
  "scripts": {
    "prebuild": "rm -rf ./build/google-material-design-icons",
    "create-icons": "node build.js --output-dir build/google-material-design-icons --svg-dir ./node_modules/material-design-icons --glob '/**/production/*_24px.svg' --renameFilter ./filters/rename/material-design-icons.js",
    "build": "npm run create-icons"
  },
  "keywords": [
    "Material Design",
    "Icons",
    "Mithril"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/mithril-icon-builder"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "contributors": ["Hai Nguyen"],
  "license": "MIT",
  "dependencies": {
    "yargs": "^3.15.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel": "^5.6.1",
    "glob": "^5.0.14",
    "lodash": "^3.10.0",
    "material-design-icons": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mustache": "^2.1.2",
    "rimraf": "^2.4.0"
  }
}
